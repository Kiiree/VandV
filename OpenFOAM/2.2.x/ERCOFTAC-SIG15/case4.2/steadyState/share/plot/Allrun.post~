#!/bin/sh
# Source tutorial run functions
. $WM_PROJECT_DIR/bin/tools/RunFunctions

latestTime=$(foamListTimes -latestTime -case ../ )

gnuplot plot/res.gp
for file in *.gp
do
    field=`basename $file .gp`
    if [ -f ../$latestTime/$field -o -f ../$latestTime/$field.gz  ];then
	gnuplot $file
    fi
done
